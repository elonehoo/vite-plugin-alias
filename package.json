{
  "name": "vite-plugin-alias",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Elone Hoo <huchengyea@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/vite-plugin-alias"
  },
  "homepage": "https://github.com/elonehoo/vite-plugin-alias#readme",
  "bugs": "https://github.com/elonehoo/vite-plugin-alias/issues",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@elonehoo/rimraf": "^0.0.1",
    "@elonehoo/vite-plugin-type-ts": "^0.0.1",
    "@types/node": "18.11.7",
    "@vitest/coverage-c8": "^0.24.3",
    "bumpp": "8.2.1",
    "c8": "^7.12.0",
    "eslint": "8.26.0",
    "pnpm": "7.14.0",
    "typescript": "4.8.4",
    "vitest": "0.24.3"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "comment-json": "^4.2.3",
    "consola": "^2.15.3",
    "fast-glob": "^3.2.12",
    "vite": "3.2.0"
  }
}
