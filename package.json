{
  "name": "vite-plugin-alias",
  "version": "0.1.0",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vite-plugin-alias#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/vite-plugin-alias"
  },
  "bugs": "https://github.com/elonehoo/vite-plugin-alias/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "play:module": "pnpm -C playground/module run dev",
    "play:common": "pnpm -C playground/common run dev",
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/eslint-config": "^0.43.0",
    "chokidar": "^3.5.3",
    "comment-json": "^4.2.3",
    "consola": "^3.2.3",
    "fast-glob": "^3.3.1",
    "unbuild": "^2.0.0",
    "vite": "4.4.9"
  },
  "devDependencies": {
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "20.6.3",
    "@vitest/coverage-c8": "^0.33.0",
    "bumpp": "9.2.0",
    "c8": "^8.0.1",
    "eslint": "8.49.0",
    "local-pkg": "^0.4.3",
    "pnpm": "8.7.6",
    "typescript": "5.2.2",
    "vitest": "0.34.4"
  }
}
