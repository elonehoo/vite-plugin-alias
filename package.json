{
  "name": "vite-plugin-alias",
  "version": "0.0.9",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vite-plugin-alias#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/vite-plugin-alias"
  },
  "bugs": "https://github.com/elonehoo/vite-plugin-alias/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "play:module": "pnpm -C playground/module run dev",
    "play:common": "pnpm -C playground/common run dev",
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "chokidar": "^3.5.3",
    "comment-json": "^4.2.3",
    "consola": "^3.2.3",
    "fast-glob": "^3.3.0",
    "unbuild": "^1.2.1",
    "vite": "4.4.2"
  },
  "devDependencies": {
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "20.4.1",
    "@vitest/coverage-c8": "^0.33.0",
    "bumpp": "9.1.1",
    "c8": "^8.0.0",
    "eslint": "8.44.0",
    "local-pkg": "^0.4.3",
    "pnpm": "8.6.7",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  }
}
