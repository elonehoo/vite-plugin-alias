{
  "name": "vite-plugin-alias",
  "version": "0.1.1",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vite-plugin-alias#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/vite-plugin-alias"
  },
  "bugs": "https://github.com/elonehoo/vite-plugin-alias/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "play:module": "pnpm -C playground/module run dev",
    "play:common": "pnpm -C playground/common run dev",
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "comment-json": "^4.2.5",
    "consola": "^3.3.3",
    "fast-glob": "^3.3.3",
    "unbuild": "^3.2.0",
    "vite": "6.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.12.1",
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "bumpp": "9.9.2",
    "c8": "^10.1.3",
    "eslint": "9.17.0",
    "local-pkg": "^0.5.1",
    "pnpm": "10.3.0",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  }
}
